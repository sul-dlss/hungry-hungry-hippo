<%= render Elements::Tables::TableComponent.new(
      id:, label:, empty_message:
    ) do |component| %>
  <% component.with_header(
       headers: ['Title', 'Collection', 'Deposit status', 'Druid', 'Last modified']
     ) %>
  <% works.each do |work| %>
    <% component.with_row(values: values_for(work), id: id_for(work)) %>
  <% end %>
<% end %>
