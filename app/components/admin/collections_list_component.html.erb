<%= render Elements::Tables::TableComponent.new(
      id:, label:, empty_message:
    ) do |component| %>
  <% component.with_header(
       headers: %w[Collection Druid Roles]
     ) %>
  <% collections.each do |collection| %>
    <% component.with_row(values: values_for(collection), id: id_for(collection)) %>
  <% end %>
<% end %>
