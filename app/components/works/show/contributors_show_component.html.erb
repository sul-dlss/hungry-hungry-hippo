<%= render Elements::Tables::TableComponent.new(id: 'contributors-table', classes: 'table-work mb-5', label: 'Contributors') do |component| %>
  <% component.with_header(headers:) %>
  <% contributors.each do |contributor| %>
    <% component.with_row(values: values_for(contributor)) %>
  <% end %>
<% end %>
